#if ($students)
<table class="table table-striped">
	<thead>
		<tr>
			<th>Student Name</th>
			<th>Student ID</th>
			<th>Checkin Time</th>
		</tr>
	</thead>
	<tbody>
	#foreach ($student in $students)
		<tr>
			<td>$student.getName()</td>
			<td>$student.getId()</td>
			<td>$student.getCheckinTime()</td>
		</tr>
	#end
	</tbody>
</table>
#end
